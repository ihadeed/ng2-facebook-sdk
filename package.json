{
  "name": "ng2-facebook-sdk",
  "version": "2.0.1",
  "description": "Facebook TypeScript SDK for Angular2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf aot dist && ngc",
    "pretest": "rm -rf aot dist && tsc",
    "test": "karma start karma.conf.js --single-run",
    "pretest:watch": "tsc",
    "test:watch": "concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "link": "cd dist && npm link",
    "docs": "zm-docs-gen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyramedia/ng2-facebook-sdk.git"
  },
  "keywords": [
    "facebook",
    "angular2",
    "sdk"
  ],
  "author": "Zyra Media Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zyramedia/ng2-facebook-sdk/issues"
  },
  "homepage": "https://github.com/zyramedia/ng2-facebook-sdk",
  "devDependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/compiler-cli": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "@zyramedia/docs-gen": "^1.1.3",
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "lodash": "^4.17.4",
    "rxjs": "5.0.1",
    "systemjs": "^0.20.11",
    "typescript": "^2.2.2",
    "zone.js": "0.7.2"
  }
}
